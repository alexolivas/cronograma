{
  "name": "cronograma-timeline",
  "version": "0.0.1-alpha.0",
  "description": "A responsive react timeline",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexolivas/cronograma.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-timeline",
    "timeline",
    "cronograma",
    "javascript",
    "time",
    "line",
    "event",
    "life",
    "story"
  ],
  "author": "Alex Olivas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexolivas/cronograma/issues"
  },
  "homepage": "https://github.com/alexolivas/cronograma#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint": "eslint '**/*.js' --ignore-pattern node_modules/",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "functions": 95,
        "lines": 95
      }
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js",
      "!<rootDir>/src/**/*.stories.js",
      "!/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|png)$": "identity-obj-proxy"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.8.2",
    "@babel/preset-react": "^7.8.0",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/react": "^5.2.8",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "semver": "^7.1.1"
  }
}
